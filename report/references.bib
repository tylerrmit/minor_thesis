	doi = {https://doi.org/10.1016/j.ufug.2015.06.006},
	issn = {1618-8667},
	journal = {Urban Forestry \& Urban Greening},
	keywords = {Green view index (GVI), Google Street View (GSV), Landscape assessment, Street greenery, Visual quality},
	number = {3},
	pages = {675-685},
	title = {Assessing street-level urban greenery using Google Street View and a modified green view index},
	url = {https://www.sciencedirect.com/science/article/pii/S1618866715000874},
	volume = {14},
	year = {2015},
	Bdsk-Url-1 = {https://www.sciencedirect.com/science/article/pii/S1618866715000874},
	Bdsk-Url-2 = {https://doi.org/10.1016/j.ufug.2015.06.006}}

