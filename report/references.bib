	issn = {0140-6736},
	journal = {The Lancet},
	number = {9838},
	pages = {219-229},
	title = {Effect of physical inactivity on major non-communicable diseases worldwide: an analysis of burden of disease and life expectancy},
	url = {https://www.sciencedirect.com/science/article/pii/S0140673612610319},
	volume = {380},
