@article{BIKESAFETY,
	abstract = { Bicycles offer a promising transportation alternative to private motor vehicles, especially in areas with congestion, poor air quality, and high fuel prices. The study on which this paper is based sought rational methods for determining the most efficient use of the available funds to improve bicycle transportation. A bicycle network analysis tool was developed to assess the networkwide level of service offered to bicyclists in a study area. The tool uses route length and measures of perceived safety to quantify the bicycle friendliness of a street network. The tool can be used to compare networks and assist with the selection of locations for bicycle facility improvements. },
	annote = {Attempted to show what factors affect cyclists' perception of safety on roads in one state in the USA.

This study surveyed cyclists about a multi-section route --- actually riding the route --- and asked then to rate the safety of each section.  It then had them watch video of rides through other routes and rate them.

It created a regression model to predict the cyclists' safety rating, with the coefficients determining how much significance was attached to various attributes.},
	author = {Michael S. Klobucar and Jon D. Fricker},
	date-modified = {2021-05-29 18:18:44 +1000},
	doi = {10.3141/2031-04},
	eprint = {https://doi.org/10.3141/2031-04},
	journal = {Transportation Research Record},
	number = {1},
	pages = {25-33},
	title = {Network Evaluation Tool to Improve Real and Perceived Bicycle Safety},
	url = {https://doi.org/10.3141/2031-04},
	volume = {2031},
	year = {2007},
	Bdsk-Url-1 = {https://doi.org/10.3141/2031-04}}

@article{CAMPBELL2019101350,
	abstract = {Street traffic sign infrastructure remains an extremely difficult asset for local government to manage due to its diverse physical structure and geographical distribution. A spatial registrar of traffic infrastructure is currently a required component of local government councils' mandatory road management plans. Recent advancements of object detection technology in machine learning have presented an automated approach for the detection and classification of street signage captured by Google's Street View (GSV) imagery. This paper explores the possibility of using deep learning to produce an autonomous system for detecting traffic signs on GSV images to assist in traffic assets monitoring and maintenance. By leveraging Google's Street View API, this research offers an economic approach of building purposeful street sign computer vision datasets. A custom object detection model was trained to detect and classify Stop and Give Way signs from images captured at intersection approaches. Considering the output detected bounding box coordinates, photogrammetry approach was applied to calculate the approximate location of each detected sign in two-dimensional geographical space. The newly located and classified street signs can be combined with relevant spatial data for implementation into an asset management system. By combining GIS and the GSV API, the process is completely scalable to any level of street sign classification scope. The experiments conducted on the road network of study area recorded a detection accuracy of 95.63% and classification accuracy of 97.82%. Our proposed automated approach to the detection and localisation of street sign infrastructure has displayed a promising potential for its use by local government authorities. Our workflow can be used to detect other traffic signs and applied to other road sections and other cities. Of primary importance, this approach takes an entirely free and open-source approach throughout. The continuation of Google's Street View program will account for the spatiotemporal representation of street sign infrastructure for the ongoing maintenance and renewal programs of this valuable asset.},
	annote = {Paper by another member of the department.

What they did is similar to part of what I need to do:
* Create a dataset of 500 images per road sign (stop, give way)
* Train a deep learning model to detect those signs in Google Street View images
* Use photogrammetry (position of bounding box + known optics and position of camera) to determine a precise location for each sign
* Compare to a ``ground truth'' database of signs in one suburb where the database was in reasonable order, and investigate differences
* Apply to another suburb where the position of signs is not already known

In this case, I'll want to detect the ``bike'' symbols on bike lanes, to distinguish bike lanes from paved shoulders etc.

I'll want to use a different model (perhaps Canny/Hough line detection in OpenCV) to detect the road lane markings, then photogrammetry to estimate the lane width.

Hopefully given they were very successful identifying the stop/give way signs, it's realistic to build an engine to find the bike lane symbols on the ground.  I'll have to build my own training dataset, but I'm sure it will be character building.},
	author = {Andrew Campbell and Alan Both and Qian (Chayn) Sun},
	date-modified = {2021-05-29 18:21:05 +1000},
	doi = {https://doi.org/10.1016/j.compenvurbsys.2019.101350},
	issn = {0198-9715},
	journal = {Computers, Environment and Urban Systems},
	keywords = {Deep learning, Object detection, Google street view (GSV) images, GIS, Traffic asset management},
	pages = {101350},
	title = {Detecting and mapping traffic signs from Google Street View images using deep learning and GIS},
	url = {https://www.sciencedirect.com/science/article/pii/S0198971519300870},
	volume = {77},
	year = {2019},
	Bdsk-Url-1 = {https://www.sciencedirect.com/science/article/pii/S0198971519300870},
	Bdsk-Url-2 = {https://doi.org/10.1016/j.compenvurbsys.2019.101350}}

@article{GREENERY2015,
	abstract = {We explored Google Street View (GSV) as a street-level, urban greenery assessment tool. Street-level greenery has long played a critical role in the visual quality of urban landscapes. This living landscape element can and should be assessed for the quality of visual impact with the GSV information, and the assessed street-level greenery information could be incorporated into urban landscape planning and management. Information on street-level views of urban greenery assessment, however, is rare or nonexistent. Planners and managers' ability to plan and manage urban landscapes effectively and efficiently is, therefore, limited. GSV is one tool that might provide street-level, profile views of urban landscape and greenery, yet no research on GSV for urban planning seems available in literature. We modified an existing Green View Index (GVI) formula and conducted a case study assessment of street greenery using GSV images in the area of East Village, Manhattan District, New York City. We found that GSV to be well suited for assessing street-level greenery. We suggest further that the modified GVI may be a relatively objective measurement of street-level greenery, and that GSV in combination with GVI may be well suited in guiding urban landscape planning and management.},
	annote = {Another paper on using Google Street View images.  This time to assess greenery at the roadside.  Probably doesn't add much beyond the other Google Street View papers already reviewed.},
