	issn = {0967-070X},
	journal = {Transport Policy},
	keywords = {Bicycling, Walking, Life expectancy, Mortality, Air pollution, Accidents},
	number = {1},
	pages = {121-131},
	title = {Benefits of shift from car to active transport},
	url = {https://www.sciencedirect.com/science/article/pii/S0967070X11001119},
	volume = {19},
	year = {2012},
